// C#

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace fizzbuzz
{
    class Program
    {
        static void Main(string[] args)
        {
            /*    F I Z Z B U Z Z
             * 
             * print numbers from 0 to 100
             * instead of numbers multiples of 3 print "fizz"
             * instead of numbers multiples of 5 print "buzz"
             * instead of numbers multiples of 3 AND 5 print "buzzfizz"
             *
             */

            for (int i = 1; i <= 100; i++)
            {
                if ((i % 3 == 0) && (i % 5 == 0))
                {
                    Console.WriteLine("fizzbuzz");
                    
                }
                else if (i % 3 == 0)
                {  
                    Console.WriteLine("fizz");
                    
                }
                else if (i % 5 == 0)
                {
                    Console.WriteLine("buzz");
                    
                }
                else
                {
                    Console.WriteLine(i);
                    
                }
            }
        }
    }
}
